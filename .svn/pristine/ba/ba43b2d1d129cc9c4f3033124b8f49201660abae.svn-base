<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- default font settings -->
    <sys:Double x:Key="DefaultFontSize">13.333</sys:Double>
    <FontFamily x:Key="DefaultFontFamily">Microsoft YaHei</FontFamily>

    <sys:Double x:Key="FixedFontSize">13.333</sys:Double>
    <FontFamily x:Key="FixedFontFamily">Microsoft YaHei</FontFamily>

    <sys:Double x:Key="ViewMaxWidth">1024</sys:Double>

</ResourceDictionary>